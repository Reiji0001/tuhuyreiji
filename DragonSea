local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()

local win = Flux:Window("Dragon Sea", "Made by Reiji ", Color3.fromRGB(255, 110, 48), Enum.KeyCode.LeftControl)
local tab = win:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")
tab:Label("VietNam on Top")
tab:Button("Buff Health (Use Golden)", "Stop at 100k - 300k Hp", function()
Flux:Notification("Bufffffffffffff!", "Alright")
_G.ConCac = true

if _G.ConCac then
repeat wait(0.1)
game:GetService("Players").LocalPlayer.Backpack.Golden.Transform.Fire:FireServer(game:GetService("Players").LocalPlayer)
until _G.ConCac == false
end
end)

local tab = win:Tab("Misc", "http://www.roblox.com/asset/?id=6023426915")

tab:Button("Get Link Discord", function()
setclipboard("https://discord.gg/jYM9cjPWVq")
end)

tab:Button("FlyGui", "Reiji So Cool", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Reiji0001/Fly-Gui/main/Fly-Gui_Source"))()
end)

